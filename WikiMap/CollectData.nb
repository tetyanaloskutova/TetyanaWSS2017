Cell[BoxData[
 RowBox[{
  RowBox[{"CreateContentFile", ":=", " ", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "uniqueArticle", "}"}], ",", " ", "\n", "\t", 
     RowBox[{
      RowBox[{"text", " ", "=", " ", 
       RowBox[{"ToString", "[", 
        RowBox[{"WikipediaData", "[", 
         RowBox[{"uniqueArticle", ",", " ", "\"\<SummaryPlaintext\>\""}], 
         "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"label", " ", "=", " ", 
       RowBox[{"topicCheck", "[", "text", "]"}]}], ";", " ", "\n", "\t", 
      RowBox[{"nouns", " ", "=", " ", 
       RowBox[{"TextCases", "[", 
        RowBox[{"text", ",", " ", "\"\<Noun\>\""}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"nouns", " ", "=", " ", 
         RowBox[{"Pluralize", "/@", " ", "nouns"}]}], ";"}], "*)"}], 
      "\[IndentingNewLine]", "\t", 
      RowBox[{"nouns", " ", "=", " ", 
       RowBox[{"DeleteStopwords", "[", 
        RowBox[{"ToLowerCase", "[", "nouns", "]"}], "]"}]}], ";", "\n", "\t", 
      RowBox[{"freqnouns", "=", 
       RowBox[{"Sort", "[", 
        RowBox[{
         RowBox[{"Counts", "[", "nouns", "]"}], ",", " ", "Greater"}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\t", 
      RowBox[{"features", " ", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "freqnouns", "]"}], "\[GreaterEqual]", "1"}],
          ",", 
         RowBox[{
          RowBox[{"Keys", "[", "freqnouns", "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", "\"\<\>\""}], "]"}]}], ";", 
      "\n", "\t", 
      RowBox[{"{", 
       RowBox[{"features", ",", " ", "label"}], "}"}]}]}], "\n", "\t", 
    "]"}]}], "\n"}]], "Code",
 InitializationCell->
  False,ExpressionUUID->"3d9f48e0-370a-4fc2-80ff-b1b5da1a2a5c"],

