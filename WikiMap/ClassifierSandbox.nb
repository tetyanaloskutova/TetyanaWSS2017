          "\[IndentingNewLine]", 
          RowBox[{"testIds", "=", 
           RowBox[{"dataIds", "~", "RandomSample", "~", "sz"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"trainIds", "=", 
           RowBox[{"dataIds", "~", "Complement", "~", "testIds"}]}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"selectedFeatures", " ", "=", 
           RowBox[{"Part", "[", 
            RowBox[{"dataFeatures", ",", " ", "trainIds"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
