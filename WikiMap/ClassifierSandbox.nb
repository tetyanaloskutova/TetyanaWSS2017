          "\[IndentingNewLine]", 
          RowBox[{"For", " ", "[", 
           RowBox[{
            RowBox[{"kj", " ", "=", " ", "1"}], ",", " ", 
            RowBox[{"kj", " ", "<=", 
             RowBox[{"Length", "[", "trainIds", "]"}]}], ",", " ", 
            RowBox[{"kj", "++"}], ",", 
            RowBox[{"(", "\[IndentingNewLine]", 
