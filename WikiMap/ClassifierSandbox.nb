          "\[IndentingNewLine]", 
          RowBox[{"testLabelsTrue", " ", "=", " ", 
           RowBox[{"Part", "[", 
            RowBox[{"dataLabels", ",", " ", "testIds"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"testLabels", " ", "=", " ", 
             RowBox[{"Map", "[", 
              RowBox[{"localClf", ",", " ", "testFeatures"}], "]"}]}], ";"}], 
           "*)"}], "\[IndentingNewLine]", 
          RowBox[{"localAccuracy", " ", "=", " ", 
           RowBox[{"ClassifierMeasurements", "[", 
            RowBox[{"localClf", ",", 
             RowBox[{"testFeatures", " ", "\[Rule]", " ", "testLabelsTrue"}], 
             ",", "\"\<Accuracy\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<Local accuracy:\>\"", " ", "~~", " ", "localAccuracy"}], 
           "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Do", " ", "the", " ", "relational", " ", "classifier"}], 
           "*)"}], "\[IndentingNewLine]", 
          RowBox[{"selectedNodes", " ", "=", " ", 
           RowBox[{"Part", "[", 
            RowBox[{"dataIds", ",", " ", "trainIds"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"selectedLinkFeatures", " ", "=", " ", 
           RowBox[{"Table", "[", 
            RowBox[{"0", ",", 
             RowBox[{"Length", "[", "trainIds", "]"}], ",", 
             RowBox[{"Length", "[", "uniqueLabels", "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"conditionalMap", " ", "=", " ", 
