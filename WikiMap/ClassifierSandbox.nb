             RowBox[{"Method", "\[Rule]", "\"\<LogisticRegression\>\""}]}], 
            "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
            RowBox[{"testRelLabels", " ", "=", " ", 
             RowBox[{"Map", "[", 
              RowBox[{"relClf", ",", "testFeatures"}], "]"}]}], ";"}], "*)"}],
           "\[IndentingNewLine]", 
          RowBox[{"testLinkFeatures", " ", "=", " ", 
           RowBox[{"Table", "[", 
            RowBox[{"0", ",", 
             RowBox[{"Length", "[", "testIds", "]"}], ",", 
