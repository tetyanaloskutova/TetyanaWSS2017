          "\[IndentingNewLine]", 
          RowBox[{"For", " ", "[", 
           RowBox[{
            RowBox[{"jj", " ", "=", " ", "1"}], ",", " ", 
            RowBox[{"jj", " ", "<=", 
             RowBox[{"Length", "[", "trainIds", "]"}]}], ",", " ", 
            RowBox[{"jj", "++"}], ",", 
            RowBox[{"(", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"selectedFeatures1", " ", "=", " ", 
               RowBox[{"aggregate", "[", 
                RowBox[{"conditionalMap", ",", " ", 
                 RowBox[{"selectedNodes", "[", 
                  RowBox[{"[", "jj", "]"}], "]"}], ",", 
